<h1>Edit fingerprint</h1>

<% form_for(@fingerprint) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :regex %><br />
    <%= f.text_field :regex %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :confidence %><br />
    <%= f.text_field :confidence %>
  </p>
  <p>
    <%= f.label :references %><br />
    <%= f.text_field :references %>
  </p>
  
  <p>
    <%= f.submit 'Save' %>
  </p>
<% end %>

<%= link_to 'Back', fingerprints_path %>
