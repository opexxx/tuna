<h1>New fingerprint</h1>

<% form_for(@fp) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :regex %><br />
    <%= f.text_area :regex %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', fingerprints_path %>
